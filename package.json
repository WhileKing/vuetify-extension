{
  "name": "vuetify-extension",
  "version": "0.2.2",
  "main": "dist/vuetify-extension.common.js",
  "description": "扩展vuetify 库, 提供常用操作类及装饰器",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build:lib": "vue-cli-service build --target lib --name vuetify-extension src/lib.js"
  },
  "keywords": [
    "vuetify",
    "vuetify-extension",
    "tool"
  ],
  "files": [
    "dist",
    "*.d.ts"
  ],
  "author": "feather",
  "license": "MIT",
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vuetify": "^2.2.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "^2.0.8",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WhileKing/vuetify-extension"
  },
  "bugs": {
    "url": "https://github.com/WhileKing/vuetify-extension/issues"
  },
  "homepage": "https://github.com/WhileKing/vuetify-extension#readme"
}
